{
  "$schema": "https://raw.githubusercontent.com/BohemianCoding/SketchAPI/develop/docs/sketch-plugin-manifest-schema.json",
  "icon": "icon.png",
  "commands": [
    {
      "script": "script.js",
      "name": "Current Document",
      "handlers": {
        "run": "copyDocumentUUID"
      },
      "identifier": "com.tmdvs.sketch.copyuuid.copydocumentuuid"
    },
    {
      "script": "script.js",
      "name": "Current Page",
      "handlers": {
        "run": "copyPageUUID"
      },
      "identifier": "com.tmdvs.sketch.copyuuid.copypageuuid"
    },
    {
      "script": "script.js",
      "name": "Selected Artboards",
      "handlers": {
        "run": "copyArtboardUUIDs"
      },
      "identifier": "com.tmdvs.sketch.copyuuid.copyartboarduuids"
    },
    {
      "script": "script.js",
      "name": "Selected Layers",
      "handlers": {
        "run": "copyLayerUUIDs"
      },
      "identifier": "com.tmdvs.sketch.copyuuid.copylayeruuids"
    }
  ],
  "menu": {
    "title": "Copy UUID",
    "items": [
      "com.tmdvs.sketch.copyuuid.copydocumentuuid",
      "com.tmdvs.sketch.copyuuid.copypageuuid",
      "com.tmdvs.sketch.copyuuid.copyartboarduuids",
      "com.tmdvs.sketch.copyuuid.copylayeruuids"
    ]
  }
}
